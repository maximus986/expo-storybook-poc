{
  "name": "expo-storybook-poc",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "storybook": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start --android",
    "build-storybook": "sb-rn-get-stories build",
    "storybook:publish": "npx chromatic"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "dotenv": "^16.0.3",
    "expo": "~48.0.9",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/datetimepicker": "^7.0.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.1",
    "@storybook/addon-ondevice-controls": "^6.5.1",
    "@storybook/react-native": "^6.5.1",
    "@types/react": "~18.0.14",
    "@types/react-native-dotenv": "^0.2.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "chromatic": "^6.17.3",
    "react-dom": "18.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "typescript": "^4.9.4"
  },
  "private": true
}
